pipeline
{
	agent any

	stages {
		stage("validetion")
		{
			steps {
				sh '''
					mvn validate
					echo "validetion is complite"
				'''
				}
		}
		stage ("bulide")
		{
			steps {
				sh '''
					mvn clean package 
					echo "package is bulide"
				'''
				}
		}
		stage ("excuting")
		{
			steps{
				sh '''
					chmod 777 java-project2-1.1-SNAPSHOT.jar
					java -jar java-project2-1.1-SNAPSHOT.jar 
				'''
			}
	}
	}
}

